{
    "test_blast_nt": {
        "type": "assert_equal",
        "args": {
            "sequence": "ATACTCAGTCACACAAGCCATAGCAGGAAACAGCGAGCTTGCAGCCTCAC",
            "limit": 1
        },
        "expected_result": [
            [
                "PREDICTED: Pan paniscus CASP8 and FADD like apoptosis regulator (CFLAR), transcript variant X12, mRNA",
                "Pan paniscus",
                "pygmy chimpanzee",
                9597,
                93.5,
                93.5,
                "100%",
                1e-15,
                "100.00%",
                6682,
                "XR_010109533.1"
            ]
        ]
    },
    "test_blast_bad_seq": {
        "type": "error",
        "args": {
            "sequence": "BANANA123"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_fasta": {
        "type": "error",
        "args": {
            "sequence": "banana.fa"
        },
        "expected_result": "FileNotFoundError"
    },
    "test_blast_bad_program": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "banana"
        },
        "expected_result": "ValueError"
    },
    "test_blast_db_missing": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "blastn"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_db1": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "blastp",
            "database": "banana"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_db2": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "database": "banana"
        },
        "expected_result": "ValueError"
    }
}
