{
    "test_blast_nt": {
        "type": "assert_equal",
        "args": {
            "sequence": "ATACTCAGTCACACAAGCCATAGCAGGAAACAGCGAGCTTGCAGCCTCACCGACGAGTCTCAACTAAAAGGGACTCCCGGAGCTAGGGGTGGGGACTCGGCCTCACACAGTGAGTGCCGG",
            "limit": 1
        },
        "expected_result": [
            [
                "Homo sapiens ATAC-STARR-seq lymphoblastoid active region 16974 (LOC129935398) on chromosome 2",
                "Homo sapiens",
                "human",
                9606,
                222,
                222,
                "100%",
                5.999999999999999e-54,
                "100.00%",
                460,
                "NG_168413.1"
            ]
        ]
    },
    "test_blast_nt_json": {
        "type": "assert_equal",
        "args": {
            "sequence": "ATACTCAGTCACACAAGCCATAGCAGGAAACAGCGAGCTTGCAGCCTCACCGACGAGTCTCAACTAAAAGGGACTCCCGGAGCTAGGGGTGGGGACTCGGCCTCACACAGTGAGTGCCGG",
            "limit": 1,
            "json": true
        },
        "expected_result": [
            {
                "Description": "Homo sapiens ATAC-STARR-seq lymphoblastoid active region 16974 (LOC129935398) on chromosome 2",
                "Scientific Name": "Homo sapiens",
                "Common Name": "human",
                "Taxid": 9606,
                "Max Score": 222,
                "Total Score": 222,
                "Query Cover": "100%",
                "E value": 6e-54,
                "Per. Ident": "100.00%",
                "Acc. Len": 460,
                "Accession": "NG_168413.1"
            }
        ]
    },
    "test_blast_bad_seq": {
        "type": "error",
        "args": {
            "sequence": "BANANA123"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_fasta": {
        "type": "error",
        "args": {
            "sequence": "banana.fa"
        },
        "expected_result": "FileNotFoundError"
    },
    "test_blast_bad_program": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "banana"
        },
        "expected_result": "ValueError"
    },
    "test_blast_db_missing": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "blastn"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_db1": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "program": "blastp",
            "database": "banana"
        },
        "expected_result": "ValueError"
    },
    "test_blast_bad_db2": {
        "type": "error",
        "args": {
            "sequence": "tests/fixtures/muscle_aa_test.txt",
            "limit": 3,
            "database": "banana"
        },
        "expected_result": "ValueError"
    }
